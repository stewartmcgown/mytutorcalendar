{
  "manifest_version": 2,

  "name": "MyTutor Calendar",
  "description": "Add MyTutor bookings to your calendar automatically.",
  "version": "3.2.1",
  
  "icons": { "16": "icon16.png",
           "32": "icon32.png",
          "128": "icon128.png",
			"256": "icon-256.png"},
  
  "content_scripts":
    [{
      "matches": ["https://www.mytutor.co.uk/tutors/secure/bookings.html"],
	  "css": ["mytutor-calendar.css","tc-toast.css"],
      "js": ["jquery.js", "mytutor-calendar.js","tc-simpletoast.js","past-prices.js"]
    }],
	
	   "key": "",
	
	"oauth2": {
		"client_id": "564036640078-4k54vkvcobikqar7kg1bmtuji5kulh82.apps.googleusercontent.com",
		"scopes":["https://www.googleapis.com/auth/calendar"
		]
	},
	
	
	"permissions": [
      "identity",
      "storage"
    ],
	
	 "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    }
	
}
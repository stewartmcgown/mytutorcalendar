{
  "manifest_version": 2,

  "name": "MyTutor Calendar",
  "description": "Add MyTutor bookings to your calendar automatically.",
  "version": "4.3",
  
  "icons": { "16": "icons/icon16.png",
           "32": "icons/icon32.png",
          "128": "icons/icon128.png",
			"256": "icons/icon256.png"},
  
  "content_scripts":
    [{
      "matches": ["https://www.mytutor.co.uk/tutors/secure/bookings.html"],
	  "css": ["content/mtc.css","content/tc-toast.css"],
      "js": ["includes/jquery.js", "content/mtc.js","content/tc-simpletoast.js"]
    }],
	
   "key": "",
	
	"oauth2": {
		"client_id": "564036640078-4k54vkvcobikqar7kg1bmtuji5kulh82.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/calendar"]
	},
	
	
	"permissions": [
    "identity",
    "storage"
  ],
	
	 "background": {
      "scripts": [
        "includes/jquery.js",
        "includes/chrome-extension-async.js",
        "background.js"
      ],
      "persistent": false
    }
	
}